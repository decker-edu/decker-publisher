{
  "name": "decker-publisher",
  "version": "0.1.0",
  "main": "dist/server",
  "private": true,
  "scripts": {
    "start": "node --trace-warnings ./dist/server.js",
    "start-static": "npm run static-dirs && npm start",
    "watch": "nodemon --watch ./src --watch ./static --ext ts,js,css --exec npm run car",
    "watch-static": "nodemon --watch ./static --ext js,css --exec npm run start-static",
    "test": "jest",
    "setup": "node --trace-warnings ./dist/setup.js",
    "static-dirs": "copyfiles \"./static/**/*\" \"./dist/\" && copyfiles -u 3 ./src/frontend/views/* ./dist/frontend/views/ && copyfiles ./config.json ./dist/",
    "build": "tsc && npm run static-dirs",
    "scribble": "ts-node scribble.ts",
    "car": "npm run build && npm run start",
    "cli": "node ./dist/cli.js"
  },
  "_moduleAliases": {
    "@root": "dist"
  },
  "dependencies": {
    "apache-md5": "^1.1.8",
    "argon2": "^0.30.3",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "email-validator": "^2.0.4",
    "escape-html": "^1.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.17.3",
    "form-data": "^4.0.0",
    "get-video-duration": "^4.1.0",
    "html-to-text": "^9.0.5",
    "http-errors": "~2.0.0",
    "module-alias": "^2.2.3",
    "morgan": "~1.10.0",
    "node-fetch": "^2.6.9",
    "node-rake": "^1.0.1",
    "pdfjs-dist": "^3.3.122",
    "pg": "^8.9.0",
    "pug": "^3.0.2",
    "showdown": "^2.1.0",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.16",
    "@types/express-fileupload": "^1.4.1",
    "@types/express-session": "^1.17.5",
    "@types/html-to-text": "^9.0.1",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.3",
    "@types/pg": "^8.6.6",
    "@types/showdown": "^2.0.0",
    "@types/yauzl": "^2.10.0",
    "@types/yazl": "^2.4.2",
    "copyfiles": "^2.4.1",
    "jest": "^29.4.1",
    "mock-stdin": "^1.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  }
}

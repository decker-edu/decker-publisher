{
  "name": "decker-publisher",
  "version": "0.1.0",
  "main": "dist/server",
  "private": true,
  "scripts": {
    "start": "npm run static-dirs && node --trace-warnings ./dist/src/server.js",
    "test": "jest",
    "setup": "node --trace-warnings ./dist/src/setup.js",
    "static-dirs": "copyfiles -u 3 \"src/frontend/static/**/*\" dist/src/frontend/static/ && copyfiles -u 3 src/frontend/views/* dist/src/frontend/views/",
    "build": "tsc && npm run static-dirs",
    "scribble": "ts-node scribble.ts",
    "car": "npm run build && npm run start"
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "email-validator": "^2.0.4",
    "escape-html": "^1.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.17.3",
    "form-data": "^4.0.0",
    "get-video-duration": "^4.1.0",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0",
    "node-fetch": "^2.6.9",
    "pdfjs-dist": "^3.3.122",
    "pg": "^8.9.0",
    "pug": "^3.0.2",
    "showdown": "^2.1.0",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.16",
    "@types/express-fileupload": "^1.4.1",
    "@types/express-session": "^1.17.5",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/node-fetch": "^2.6.3",
    "@types/pg": "^8.6.6",
    "@types/showdown": "^2.0.0",
    "@types/yauzl": "^2.10.0",
    "@types/yazl": "^2.4.2",
    "copyfiles": "^2.4.1",
    "jest": "^29.4.1",
    "mock-stdin": "^1.0.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1"
  }
}
